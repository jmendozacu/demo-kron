<?php $url = Mage::helper('madapter')->getConfig('link_alert'); ?>
<?php $message = Mage::helper('madapter')->getConfig('content_alert'); ?>
<script type="text/javascript">
    document.observe("dom:loaded", function() {
        // initially hide all containers for tab content
        var r=confirm("<?php echo $message;?>");
        if (r==true)
        {
            setLocation('<?php echo $url; ?>');
        }       
    });            

</script>